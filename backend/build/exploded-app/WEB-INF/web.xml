<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
   <servlet>
        <servlet-name>SystemServiceServlet</servlet-name>
        <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
        <init-param>
            <param-name>services</param-name>
            <param-value>com.example.dgduncan.myapplication.backend.Endpoints.AnimalEndpoint,
            com.example.dgduncan.myapplication.backend.Endpoints.TrackEndpoint</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>SystemServiceServlet</servlet-name>
        <url-pattern>/_ah/spi/*</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>SharkUpdateTest</servlet-name>
        <servlet-class>com.example.dgduncan.myapplication.backend.Cron.SharkUpdate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SharkUpdateTest</servlet-name>
        <url-pattern>/sharkupdatetest</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TrackUpdate</servlet-name>
        <servlet-class>com.example.dgduncan.myapplication.backend.Cron.TrackUpdate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TrackUpdate</servlet-name>
        <url-pattern>/trackupdate</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TrackUpdateTest</servlet-name>
        <servlet-class>com.example.dgduncan.myapplication.backend.Cron.TrackUpdateTest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TrackUpdateTest</servlet-name>
        <url-pattern>/trackupdatetest</url-pattern>
    </servlet-mapping>

    <filter>
        <filter-name>ObjectifyFilter</filter-name>
        <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>ObjectifyFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
</web-app>